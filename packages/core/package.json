{
  "name": "0xbitnet",
  "version": "0.1.0",
  "description": "Run BitNet b1.58 ternary LLMs with WebGPU",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./internals": {
      "types": "./dist/internals.d.ts",
      "import": "./dist/internals.js",
      "require": "./dist/internals.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "bitnet",
    "webgpu",
    "llm",
    "inference",
    "ternary",
    "1-bit",
    "transformer"
  ],
  "license": "MIT",
  "devDependencies": {
    "@webgpu/types": "^0.1.52",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
